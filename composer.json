{
  "require": {
    "craftcms/cms": "3.0.37",
    "vlucas/phpdotenv": "^2.4.0",
    "wbrowar/adminbar": "3.1.6",
    "craftcms/redactor": "2.1.7"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
